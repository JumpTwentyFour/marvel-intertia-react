{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=8",
        "caseysoftware/marvel-helper": "^2.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "inertiajs/inertia-laravel": "^0.3.5",
        "laravel/framework": "^8.1",
        "laravel/helpers": "^1.2",
        "mad-web/laravel-initializer": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.9.0",
        "beyondcode/laravel-dump-server": "^1.0",
        "brianium/paratest": "^6.1.2",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "filp/whoops": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.17",
        "laravel/tinker": "^2.2.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "nunomaduro/larastan": "^0.6.11",
        "php-parallel-lint/php-parallel-lint": "^1.0",
        "phpro/grumphp": "^1.3.0",
        "phpstan/phpstan": "^0.12.14",
        "phpunit/phpunit": "^9.0",
        "roave/security-advisories": "dev-master",
        "slevomat/coding-standard": "^6.1",
        "squizlabs/php_codesniffer": "^3.4",
        "sustainabil-it/phpstan-higher-order-collections": "dev-main"
    },
    "autoload": {
        "classmap": [
            "database/seeders",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "project-install": [
            "cp .env.example .env",
            "@composer install"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ],
        "grumphp-deinit": [
            "@php vendor/bin/grumphp git:deinit"
        ],
        "grumphp-init": [
            "@php vendor/bin/grumphp git:init"
        ],
        "lint": [
            "vendor/bin/parallel-lint ./ --blame --exclude vendor _ide_helper.php .phpstorm.meta.php"
        ],
        "lint:fix": [
            "vendor/bin/phpcbf --standard=./phpcs.xml --extensions=php --ignore=*.blade.php,database/*,vendor/*,_ide_helper.php,.phpstorm.meta.php"
        ],
        "lint:parallel": [
            "vendor/bin/parallel-lint ./ --blame --exclude vendor _ide_helper.php .phpstorm.meta.php"
        ],
        "phpstan": [
            "vendor/bin/phpstan analyse --memory-limit=4G"
        ],
        "phpunit": [
            "vendor/bin/phpunit"
        ],
        "test": [
            "Composer\\Config::disableProcessTimeout",
            "php artisan cache:clear",
            "php artisan test"
        ],
        "test:pipeline": [
            "Composer\\Config::disableProcessTimeout",
            "phpunit -d memory_limit=2048M --colors=always --log-junit ./test-reports/junit.xml"
        ],
        "phpcs": [
            "vendor/bin/phpcs --standard=./phpcs.xml --extensions=php ."
        ],
        "analyse": [
            "@phpcs",
            "@phpstan"
        ],
        "project-install-local": [
            "cp .env.local .env",
            "@composer install",
            "@php artisan app:install"
        ],
        "project-update-local": [
            "@php artisan app:update"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
